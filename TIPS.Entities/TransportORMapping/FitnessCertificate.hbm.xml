<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="TIPS.Entities.TransportEntities"
                   assembly="TIPS.Entities.TransportEntities.FitnessCertificate">

  <class name="TIPS.Entities.TransportEntities.FitnessCertificate, TIPS.Entities" table="FitnessCertificate" dynamic-update="true">
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="Int64">
      <generator class="native"/>
    </id>
    <property name="RefId"/>
    <property name="VehicleId"/>
    <property name="Type"/>
    <property name="VehicleNo"/>
    <property name="FCDate" type="datetime"/>
    <property name="NextFCDate" type="datetime"/>
    <property name="FCCost"/>
    <property name="Description"/>
    <property name="FCWorkCarriedAt"/>
    <property name="RTO"/>
    <property name="Driver"/>
    <property name="CreatedDate" column="CreatedDate" type="datetime"></property>
    <property name="CreatedBy" column="CreatedBy" type="string"></property>
    <property name="FCertificate" />
    <property name="FCTaxValidUpto" />
  </class>
</hibernate-mapping>
