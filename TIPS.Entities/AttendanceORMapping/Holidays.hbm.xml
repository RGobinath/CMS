<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="TIPS.Entities.Attendance.Holidays" namespace="TIPS.Entities.Attendance" xmlns="urn:nhibernate-mapping-2.2">
  <class name="TIPS.Entities.Attendance.Holidays, TIPS.Entities" table="Holidays" lazy="true" >
    <id name="Id" column="Id" type="Int64">
      <generator class="native" />
    </id>
    <property name="Holiday" column="Holiday" type="datetime" />
    <property name="Name" column="Name" type="string" />
    <property name="Comments" column="Comments" type="string" />
    <property name="Campus" column="Campus" type="string" />
    <property name="Grade" column="Grade" type="string" />
    <property name="AcademicYear" column="AcademicYear" type="string" />
  </class>
</hibernate-mapping>