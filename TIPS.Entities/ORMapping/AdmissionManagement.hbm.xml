<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TIPS.Entities" namespace="TIPS.Entities" >
  <class name="AdmissionManagement" table="AdmissionManagement" lazy="false">
    <id name="ApplicationNo">
      <column name="ApplicationNo"/>
      <generator class="identity"/>
    </id>
    <property name="PreRegNo">
      <column name="PreRegNo" sql-type="string"/>
    </property>
    <property name="ApplicantName">
      <column name="ApplicantName" sql-type="string"/>
    </property>
    <property name="Grade">
      <column name="Grade" sql-type="string"/>
    </property>
    <property name="Status">
      <column name="Status" sql-type="string"/>
    </property>
    
    <property name="AppliedDate">
      <column name="AppliedDate" sql-type="datetime"/>
    </property>

   
  </class>
</hibernate-mapping>