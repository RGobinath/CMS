      <column name="RoleName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedBy">
      <column name="CreatedBy" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ModifiedBy">
      <column name="ModifiedBy" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ModifiedDate">
      <column name="ModifiedDate" sql-type="nvarchar" not-null="false" />
