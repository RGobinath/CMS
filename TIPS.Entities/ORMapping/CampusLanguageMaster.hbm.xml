<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TIPS.Entities" namespace="TIPS.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="CampusLanguageMaster" table="CampusLanguageMaster" lazy="true" >
    <id name="Id" column="Id" type="Int32">
      <generator class="native" />
    </id>
    <property name="LanguageName" column="LanguageName" type="AnsiString" not-null="false" />
    <property name="Description" column="Description" type="AnsiString" not-null="false" />
    <property name="Campus" column="Campus" type="AnsiString" not-null="false" />
    <property name="Grade" column="Grade" type="AnsiString" not-null="false" />
  </class>
</hibernate-mapping>