<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="TIPS.Entities.AcademicyrMaster" namespace="TIPS.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="TIPS.Entities.AcademicyrMaster, TIPS.Entities" table="ACADEMICMASTER" lazy="false" >
    <id name="FormId" column="FormId">
      <generator class="identity" />
    </id>
    <property name="FormCode" column="FormCode" type="string" not-null="true" />
    <property name="AcademicYear" column="acad" type="string" not-null="true" />
    <property name="AcademicYeardesc" column="acaddesc" type="string" not-null="true" />

    <!--Ace-->
    <property name="CreatedBy" column="CreatedBy" type="string"/>
    <property name="CreatedDate" column="CreatedDate" type="datetime"/>
    <property name="UpdateBy" column="UpdateBy" type="string"/>
    <property name="UpdateDate" column="UpdateDate" type="datetime"/>
   <property name="IsActive" column="IsActive" type="bool"/>
  </class>
</hibernate-mapping>


