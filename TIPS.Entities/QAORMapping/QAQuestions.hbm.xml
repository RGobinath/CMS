    <property name="EscalationPeriod" column="EscalationPeriod" type="Int64"  not-null ="false"></property>
    <property name="QuestionTitle" column="QuestionTitle" type="string"></property>
    <property name="Replies" column="Replies" type="Int64"  ></property>
    <property name="Views" column="Views" type="Int64"  ></property>
    <property name="Campus" column="Campus" type="string"></property>
