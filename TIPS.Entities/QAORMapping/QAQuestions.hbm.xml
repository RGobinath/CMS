    <property name="QuestionRefId" column="QuestionRefId" type="string"></property>
    <property name="ExpiryDate" column="ExpiryDate" type="datetime"></property>
    <property name="Status" column="Status" type="string"></property>
    <property name="ExpiryStatus" column="ExpiryStatus" type="string"></property>

    <property name="ModifiedBy" column="ModifiedBy" type="string"></property>
    <property name="ModifiedDate" column="ModifiedDate" type="datetime"></property>

  </class>
</hibernate-mapping>
