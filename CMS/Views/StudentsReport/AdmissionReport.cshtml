@{
    ViewBag.Title = "AdmissionReport";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<!-- #section:basics/navbar.layout -->
<!-- /section:basics/navbar.layout -->
<!-- /For FusionCharts -->


<script type="text/javascript">
    try { ace.settings.check('main-container', 'fixed') } catch (e) { }
</script>
<div class="page-content">
    <span>@Html.Raw(ViewBag.BreadCrumb)</span>
    <!-- /section:settings.box -->
    <div class="page-content-area">
        <!--Search Pannel-->
        <div class="row">
            <div class="col-xs-12 widget-container-col">
                <div class="widget-box widget-color-CustomizeBlue">
                    <div class="widget-header widget-header-flat widget-header-small">
                        <div class="PanelHeader">
                            <i class="ace-icon fa fa-search"></i>&nbsp;&nbsp;Search Panel</div>
                        <div class="widget-toolbar">
                            <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                            </a>
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main no-padding">
                            <div class="row" style="margin-top: 10px; margin-left: 3px; margin-bottom: 5px;">
                                <div class="col-xs-12">
                                    <div class="col-sm-2">
                                        <b>Campus </b>
                                        @Html.DropDownList("Campus", new SelectList(Enumerable.Empty<SelectListItem>()), new { @Class = "form-control select-sm" })
                                        @Html.Hidden("BackUrl", Url.Action("AdmissionReport", "StudentsReport"))
                                    </div>
                                    <div class="col-sm-2">
                                        <b>Year </b>
                                        @Html.DropDownList("ddlyear", new SelectList(ViewBag.acadddl, "AcademicYear", "AcademicYear"), "Select One", new { @Class = "form-control select-sm" })
                                        @Html.Hidden("CurrYear", "", new { id = "CurrYear", @Value = ViewBag.CurYear })                     
                                    </div>
                                    <div class="col-sm-1">
                                        <br />
                                        <button class="btn btn-success btn-block btn-sm" type="submit" id="btnSearch" value="Search">
                                            <i class="ace-icon fa fa-search white"></i>Search</button>&nbsp;&nbsp;
                                    </div>
                                    <div class="col-sm-1">
                                        <br />
                                        <button class="btn btn-danger btn-block btn-sm" type="button" id="btnReset" value="Reset">
                                            <i class="ace-icon fa fa-undo white"></i>Reset</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="space-2">
        </div>
        <div class="space-2">
        </div>
        <!-- PAGE CONTENT BEGINS -->
        <div class="row">
            <div class="col-xs-12">
                <div class="row">
                    <div class="col-sm-7">
                        <table id="AddmissionStatusReport">
                        </table>
                        <div id="AddmissionStatusReportPager">
                        </div>
                    </div>
                    <div class="col-sm-5">
                        <div id="MultiSeriesChart">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="@Url.Content("~/Scripts/FusionCharts1/FusionCharts.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/StudentReport/AdmissionReport.js")" type="text/javascript"></script>
