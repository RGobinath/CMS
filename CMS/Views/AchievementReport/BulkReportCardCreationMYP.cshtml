@{
    ViewBag.Title = "BulkReportCardCreationMYP";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<!-- #section:basics/navbar.layout -->
<!-- /section:basics/navbar.layout -->
<script type="text/javascript">
    try { ace.settings.check('main-container', 'fixed') } catch (e) { }
</script>
<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
        <span>@Html.Raw(ViewBag.BreadCrumb)</span>
        <div class="row">
            <div class="space-2">
            </div>
            <div class="col-xs-12 widget-container-col">
                <div class="row">
                    <div class="col-sm-3">
                        <div class="widget-box widget-color-CustomizeBlue">
                            <div class="widget-header widget-header-flat widget-header-small">
                                <div class="PanelHeader">
                                    <i class="ace-icon fa fa-search"></i>&nbsp;Bulk Report Card Creation</div>
                                <div class="widget-toolbar">
                                    <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="widget-body">
                                <div class="widget-main no-padding">
                                    <div class="space-4">
                                    </div>
                                    <div class="row">
                                        <div class="col-xs-12" style="margin-bottom: 5px; margin-top: 10px;">
                                            <div class="col-sm-5">
                                                <b>Campus</b>
                                            </div>
                                            <div class="col-sm-7">
                                                @Html.DropDownList("Campus", new SelectList(Enumerable.Empty<SelectListItem>()), "Select One", new { @Class = "form-control select-sm" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="space-2">
                                    </div>
                                    <div class="row">
                                        <div class="col-xs-12" style="margin-bottom: 5px">
                                            <div class="col-sm-5">
                                                <b>Grade</b>
                                            </div>
                                            <div class="col-sm-7">
                                                @Html.DropDownList("Grade",
                                            new SelectList(new[] { 
                                                new { Text = "", Value = "Select", Seleced = "True" },
                                                new { Text = "VI", Value = "VI", Seleced = "false" },
                                                new { Text = "VII", Value = "VII", Seleced = "false" },
                                                new { Text = "VIII", Value = "VIII", Seleced = "false" },
                                                new { Text = "IX", Value = "IX", Seleced = "false" },
                                                new { Text = "X", Value = "X", Seleced = "false" },
                                                new { Text = "XI", Value = "XI", Seleced = "false" },
                                                new { Text = "XII", Value = "XII", Seleced = "false" }
                                                }, "Text", "Value", "Seleced"),
                                                new { @Class = "form-control select-sm" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="space-2">
                                    </div>
                                    <div class="row">
                                        <div class="col-xs-12" style="margin-bottom: 5px">
                                            <div class="col-sm-5">
                                                <b>Semester</b>
                                            </div>
                                            <div class="col-sm-7">
                                                @Html.DropDownList("semester",
                                                new SelectList(new[] { 
                                                new { Text = "", Value = "Select", Seleced = "True" },
                                                new { Text = "Sem I", Value = "Sem I", Seleced = "false" },
                                                new { Text = "Sem II", Value = "Sem II", Seleced = "false" },
                                                }, "Text", "Value", "Seleced"),
                                                new { @Class = "form-control select-sm" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="space-2">
                                    </div>
                                    <div class="row">
                                        <div class="col-xs-12" style="margin-bottom: 5px">
                                            <div class="col-sm-5">
                                                <b>Academic Year</b>
                                            </div>
                                            <div class="col-sm-7">
                                                <select class="form-control select-sm" id="AcademicYear">
                                                    @foreach (var datelist in ViewBag.acadddl)
                                                    {
                                                        <option value="@datelist">@datelist</option>
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="space-2">
                                    </div>
                                    <div class="row">
                                        <div class="col-xs-12" style="margin-bottom: 5px">
                                            <div class="col-sm-5">
                                                <b>Section</b>
                                            </div>
                                            <div class="col-sm-7">
                                                @Html.DropDownList("Section",
                                                    new SelectList(new[] { 
                                                        new { Text = "", Value = "Select", Seleced = "True" },
                                                        new { Text = "A", Value = "A", Seleced = "false" },
                                                        new { Text = "B", Value = "B", Seleced = "false" },
                                                        new { Text = "C", Value = "C", Seleced = "false" },
                                                        new { Text = "D", Value = "D", Seleced = "false" },
                                                        new { Text = "E", Value = "E", Seleced = "false" },
                                                        new { Text = "F", Value = "F", Seleced = "false" }
                                               }, "Text", "Value", "Seleced"),
                                                        new { @Class = "form-control select-sm" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="space-2">
                                    </div>
                                    <div class="row">
                                        <div class="col-xs-12" style="margin-bottom: 5px">
                                            <div class="col-sm-5">
                                                <b>Teacher Name</b>
                                            </div>
                                            <div class="col-sm-7">
                                                @Html.TextBox("txtTeachName", "", new { @Class = "form-control input-sm" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="space-2">
                                    </div>
                                    <div class="row">
                                        <div class="col-xs-12" style="margin-bottom: 5px">
                                            <div class="col-xs-5">
                                                <b>Date</b>
                                            </div>
                                            <div class="col-sm-7">
                                                <div class="row">
                                                    <div class="col-xs-11">
                                                        @Html.TextBox("txtDate", "", new { @class = "form-control date-picker" })
                                                    </div>
                                                    <div class="col-xs-1" style="margin-left: -20px;">
                                                        <i class="ace-icon fa fa-calendar"></i>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="space-4">
                                    </div>
                                    <div class="row" style="margin-bottom: 15px;">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <button type="submit" class="btn btn-success btn-block btn-sm" id="btnsearch">
                                                    <i class="ace-icon fa fa-search white"></i>Search
                                                </button>
                                            </div>
                                            <div class="col-sm-6">
                                                <button type="button" class="btn btn-danger btn-block btn-sm" id="btnreset">
                                                    <i class="ace-icon fa fa-undo white"></i>Reset
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-2">
                                            </div>
                                            <div class="col-sm-7" style="margin-left: 10px;">
                                                <button type="button" class="btn btn-primary btn-block btn-sm" id="btnass360">
                                                    <i class="ace-icon fa fa-plus white"></i>Report Card Creation
                                                </button>
                                            </div>
                                            <div class="col-sm-3">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="space-6">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="space-2">
                    </div>
                    <div class="col-sm-9" id="jqgrid">
                        <div class="row">
                            <div class="col-xs-12">
                                <table id="BulkRptCreation">
                                </table>
                                <div id="BulkRptCreationPager">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">    window.jQuery || document.write("<script src='../assets/js/jquery.min.js'>"
+ "<" + "/script>"); </script>
<script type="text/javascript">    if ('ontouchstart'
in document.documentElement) document.write("<script src='../assets/js/jquery.mobile.custom.min.js'>"
+ "<" + "/script>"); </script>
<script src="@Url.Content("~/assets/js/AceJQMsg.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Assess360/BulkReportCardCreationMYP.js")" type="text/javascript"></script>

