            // Allow: backspace, delete, tab, escape, enter and .
            if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
                // Allow: Ctrl+A, Command+A
                (e.keyCode === 65 && (e.ctrlKey === true || e.metaKey === true)) ||
                // Allow: home, end, left, right, down, up
                (e.keyCode >= 35 && e.keyCode <= 40)) {
                // let it happen, don't do anything
                return;
            }
            // Ensure that it is a number and stop the keypress
            if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                e.preventDefault();
            }
        });
        $('#txtname,#txtfathername,#txtspousename,#txtemergencycontactperson,#fr1c2,#fr1c5,#fr2c2,#fr2c5,#fr3c2,#fr3c5,#fr4c2,#fr4c5,#rer1c2,#rer2c2').keydown(function (e) {
            if (e.ctrlKey || e.altKey) {
                e.preventDefault();
            } else {
                var key = e.keyCode;                
                if (key === 9 || (key == 28) || (key == 29) || (key == 48) || (key == 57)) {
                    return;
                }
                if (!((key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
                    e.preventDefault();
                }
            }
        });
        $("#emr1c2,#emr2c2,#emr3c2,#emr4c2,#emr5c2,#emr6c2,#emr7c2").keydown(function (e) {
            if (e.ctrlKey || e.altKey) {
                e.preventDefault();
            } else {
                var key = e.keyCode;
                if (key === 9 || (key === 28) || (key === 29) || (key === 48) || (key === 57) || (key === 188) ||( key === 190)) {
                    return;
                }
                if (!((key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
