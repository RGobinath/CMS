    function readURL(input) {
        var file = input.files[0];
        var fileSize = input.files[0].size;
        var fileType = file["type"];
        var ValidImageTypes = ["image/jpeg"];
        var maxFileSize = 2048000;
        if ($.inArray(fileType, ValidImageTypes) < 0) {
            document.getElementById("imgupload").src = "/Images/no_image.jpg";
            document.getElementById("OpenImageUpload").src = "/Images/no_image.jpg";
            ErrMsg("Invalid image or file");
            return false;
        }
        if (fileSize > maxFileSize) {
            document.getElementById("imgupload").src = "/Images/no_image.jpg";
            document.getElementById("OpenImageUpload").src = "/Images/no_image.jpg";
            ErrMsg("Image must be less than 2 MB.");
            return false;
        }
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                $('#OpenImageUpload')
                    .attr('src', e.target.result)
                    .width(130)
                    .height(130);
            };
            reader.readAsDataURL(input.files[0]);
        }
    }
