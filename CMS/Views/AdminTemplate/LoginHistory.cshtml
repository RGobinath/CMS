@model TIPS.Entities.AdminTemplateEntities.AdminTemplateTransportReportByCampus_vw
@{
    Layout = "~/Views/Shared/NewAcePartialViewMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<script type="text/javascript">
    try { ace.settings.check('main-container', 'fixed') } catch (e) { }
</script>
<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
        <div class="row" style="padding-top: 3px; padding-left:50px;">
            <div class="col-xs-12 infobox-container">
                <div class="col-sm-2">
                    <div class="infobox infobox-DarkGreen">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-book"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@ViewBag.Login_CMSUsersCount</span>
                            <div class="infobox-content">
                                CMS User</div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="infobox infobox-OrangeRed">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-pencil-square-o"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@ViewBag.Login_ParentCount</span>
                            <div class="infobox-content">
                                Parents</div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="infobox infobox-DeepPink">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-comments"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@ViewBag.Login_StaffCount</span>
                            <div class="infobox-content">
                                Staff</div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="infobox infobox-indigo">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-briefcase"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@ViewBag.Login_StudentCount</span>
                            <div class="infobox-content">
                                Students</div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="infobox infobox-ForestGreen">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-inbox"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@ViewBag.Login_OthersCount</span>
                            <div class="infobox-content">
                                Others</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="space-4"></div>
        <div class="row" style="padding-top: 10px;">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <div id="LoginBrowserChart">
                </div>
            </div>
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <div id="LoginUsertTypeChart">
                </div>
            </div>
        </div>
         <div class="space-8">
        </div>
        <div class="space-8">
        </div>
        <div class="space-8">
        </div>
        <div class="space-8">
        </div>
        <div class="space-8">
        </div>
        <div class="space-8">
        </div>
    </div>
    <!-- /.page-content-area -->
</div>
<script type="text/javascript">    if ('ontouchstart' in document.documentElement)
        document.write("<script src='../assets/js/jquery.mobile.custom.min.js'>" + "<" +
