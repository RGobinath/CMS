@model TIPS.Entities.AdminTemplateEntities.AdminTemplateAssess360MarkCount_vw
@{
    Layout = "~/Views/Shared/NewAcePartialViewMaster.cshtml";
}
<div>
    <div class="row">
        <div class="col-sm-12" style="font-size: 10px;" align="right">
            <div class="col-xs-4">
            </div>
            <div class="col-xs-2">
                <label for="form-field-select-1">
                    <b>Select Campus</b></label></div>
            <div class="col-xs-2">
                @Html.DropDownListFor(model => model.Campus, new SelectList(Enumerable.Empty<SelectListItem>()), "", new { @Class = "form-control select-sm", id = "AssessDdlCampusList" })
            </div>
            <div class="col-xs-2">
                <label for="form-field-select-1">
                    <b>Select Grade</b></label></div>
            <div class="col-xs-2">
                @Html.DropDownListFor(Model => Model.Grade, new SelectList(ViewBag.gradeddl1, "gradcod", "gradcod"), "Select One", new { id = "AssessDdlgrade", @Class = "form-control select-sm" })
            </div>
        </div>
    </div>
    <div class="space-8"></div>
    <div class="row" style="padding-top: 3px;">
        <div class="col-lg-6 col-sm-6 col-xs-12">
            <table align="center">
                <tr style="font-family: Verdana; font-size: 11px;">
                    <td align="center">
                        First Rank
                    </td>
                    <td align="center">
                        Second Rank
                    </td>
                    <td align="center">
                        Third Rank
                    </td>
                </tr>
                <tr>
                    <td>
                        <div id="AssessFirstRank">
                        </div>
                    </td>
                    <td>
                        <div id="AssessSecondRank">
                        </div>
                    </td>
                    <td>
                        <div id="AssessThirdRank">
                        </div>
                    </td>
                </tr>
            </table>
        </div>
        <div class="col-lg-6 col-sm-6 col-xs-12">
            <div id="AssessRankChart">
            </div>
        </div>
    </div>
    <div class="space-8"></div>
    <br />
    <br />
    <div class="row">
        <div class="col-lg-12 col-sm-12 col-xs-12" align="center">
            <p style="font-family: Verdana; font-size: 11px;">
                Mark Wise Students Report</p>
            <div class="col-sm-12 infobox-container">
                <div class="infobox infobox-Olive">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-book"></i>
                    </div>
                    <div class="infobox-data">
                        <span class="infobox-data-number" id="Below75Count"></span>
                        <div class="infobox-content" style="font-size: 9px;">
                            Below 75</div>
                    </div>
                </div>
                <div class="infobox infobox-OrangeRed">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-book"></i>
                    </div>
                    <div class="infobox-data">
                        <span class="infobox-data-number" id="MeritListCount"></span>
                        <div class="infobox-content" style="font-size: 9px;">
                            Merit List Count</div>
                    </div>
                </div>
                <div class="infobox infobox-DarkMegenta">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-book"></i>
                    </div>
                    <div class="infobox-data">
                        <span class="infobox-data-number" id="HightAcieversClubCount"></span>
                        <div class="infobox-content" style="font-size: 9px;">
                            Hight Acievers</div>
                    </div>
                </div>
                <div class="infobox infobox-ForestGreen">
                    <div class="infobox-icon">
                        <i class="ace-icon fa fa-book"></i>
                    </div>
                    <div class="infobox-data">
                        <span class="infobox-data-number" id="ChairmanAwardCount"></span>
                        <div class="infobox-content" style="font-size: 9px;">
                            Chairman Award</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="space-8"></div>
    <div class="space-8"></div><div class="space-8"></div><div class="space-8"></div><div class="space-8"></div>
     <div class="space-6">
    </div>
</div>
<script src="../../Scripts/AdminTemplateScripts/Assessment.js" type="text/javascript"></script>
