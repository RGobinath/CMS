@{
    Layout = "~/Views/Shared/NewAcePartialViewMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<script type="text/javascript">
    try { ace.settings.check('main-container', 'fixed') } catch (e) { }
</script>
<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
        <div class="row" style="padding-top: 3px; padding-left: 50px;">
            <div class="row">
                <div class="col-xs-12 infobox-container">
                    <div class="col-sm-2">
                        <div class="infobox infobox-red">
                            <div class="infobox-icon">
                                <i class="ace-icon fa fa-user"></i>
                            </div>
                            <div class="infobox-data">
                                <span class="infobox-data-number">@ViewBag.Dash_TotalUsersCount</span>
                                <div class="infobox-content">
                                    No of Users</div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-2">
                        <div class="infobox infobox-DarkGreen">
                            <div class="infobox-icon">
                                <i class="ace-icon fa fa-book"></i>
                            </div>
                            <div class="infobox-data">
                                <span class="infobox-data-number">@ViewBag.Dash_TotalStaffCount</span>
                                <div class="infobox-content">
                                    Staff</div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-2">
                        <div class="infobox infobox-indigo">
                            <div class="infobox-icon">
                                <i class="ace-icon fa fa-globe"></i>
                            </div>
                            <div class="infobox-data">
                                <span class="infobox-data-number">@ViewBag.Dash_TotalCampusCount</span>
                                <div class="infobox-content">
                                    Campus</div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-2">
                        <div class="infobox infobox-emerald">
                            <div class="infobox-icon">
                                <i class="ace-icon fa fa-male"></i>
                            </div>
                            <div class="infobox-data">
                                <span class="infobox-data-number">@ViewBag.Dash_TotalStudentsCount</span>
                                <div class="infobox-content">
                                    Students</div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-2">
                        <div class="infobox infobox-orange2">
                            <div class="infobox-icon">
                                <i class="ace-icon fa fa-eye"></i>
                            </div>
                            <div class="infobox-data">
                                <span class="infobox-data-number">@ViewBag.Dash_TotalLoginCount</span>
                                <div class="infobox-content">
                                    Total Login</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row" align="center">
                <div id="DashBoardIndexCampusChart">
                </div>
            </div>
            <div class="space-8">
            </div>
        </div>
    </div>
    <!-- /.page-content-area -->
</div>
<script type="text/javascript">    if ('ontouchstart' in document.documentElement)
        document.write("<script src='../assets/js/jquery.mobile.custom.min.js'>" + "<" +
"/script>"); </script>
<!-- inline scripts related to this page -->
<script src="../../Scripts/AdminTemplateScripts/DashBoardIndex.js" type="text/javascript"></script>
