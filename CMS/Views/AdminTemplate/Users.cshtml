@model TIPS.Entities.AdminTemplateEntities.AdminTemplateTransportReportByCampus_vw
@{
    Layout = "~/Views/Shared/NewAcePartialViewMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<script type="text/javascript">
    try { ace.settings.check('main-container', 'fixed') } catch (e) { }
</script>
<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
        <div class="row" style="padding-left:50px;">
            <div class="col-xs-12 infobox-container">
                <div class="col-sm-2">
                    <div class="infobox infobox-Olive">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-user"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@ViewBag.Users_CMSUsersCount</span>
                            <div class="infobox-content">
                                CMS Users</div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="infobox infobox-OrangeRed">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-puzzle-piece"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@ViewBag.Users_ParentCount</span>
                            <div class="infobox-content">
                                Parents</div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="infobox infobox-indigo">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-qrcode"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@ViewBag.Users_StaffCount</span>
                            <div class="infobox-content">
                                Staff</div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="infobox infobox-DeepPink">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-male"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@ViewBag.Users_StudentCount</span>
                            <div class="infobox-content">
                                Students</div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="infobox infobox-DarkMegenta">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-ticket"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@ViewBag.Users_OthersCount</span>
                            <div class="infobox-content">
                                Other Users</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <div id="UsersCampusWiseChart">
                </div>
            </div>
            <div class="col-lg-6 col-sm-6 col-xs-12">
                <div id="UserTypeChart">
                </div>
            </div>
        </div>
        <div class="space-8">
        </div>
        <div class="space-8">
        </div>
        <div class="space-8">
        </div>
        <div class="space-8">
        </div>
        <div class="space-8">
        </div>
        <div class="space-8">
        </div>
    </div>
    <!-- /.page-content-area -->
</div>
