@model TIPS.Entities.TransportEntities.DriverMaster
<div class="row">
    <div class="col-sm-12">
        <div id="PersonalAccordion" class="accordion-style1 panel-group">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title">
                        <a class="accordion-toggle" data-toggle="collapse" data-parent="#PersonalAccordion"
                            href="#GeneralDetails"><i class="ace-icon fa fa-angle-down bigger-110" data-icon-hide="ace-icon fa fa-angle-down"
                                data-icon-show="ace-icon fa fa-angle-right"></i>&nbsp;Other Details </a>
                    </h4>
                </div>
                <div class="panel-collapse collapse in" id="GeneralDetails">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="col-sm-3">
                                    <label for="form-field-select-1">
                                        <b>Years Of Exp in Driving</b>
                                    </label>
                                    @Html.TextBoxFor(model => model.TotalYearsOfExp, new { id = "totalexp", @Class = "form-control input-sm", @maxlength = "10" })
                                </div>
                                <div class="col-sm-3">
                                    <label for="form-field-select-1">
                                        <b>License no</b></label>
                                    @Html.TextBoxFor(model => model.LicenseNo, new { id = "licenseNo", @Class = "form-control input-sm", @maxlength = "60" })
                                </div>
                                <div class="col-sm-3">
                                    <label for="form-field-select-1">
                                        <b>License Valid Date</b></label>
                                    @Html.TextBoxFor(model => model.LicenseValDate, new { id = "LicenseValDate", @Class = "form-control Attendancedatepicker" })
                                </div>
                                <div class="col-sm-3">
                                    <label for="form-field-select-1">
                                        <b>NonTransport License Valid Date</b></label>
                                    @Html.TextBoxFor(model => model.NonTraLicenseValDate, new { id = "nonTrnLcn", @Class = "form-control Attendancedatepicker" })
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-sm-3">
                                    <label for="form-field-select-1">
                                        <b>Batch Number</b></label>
                                    @Html.TextBoxFor(model => model.BatchNo, new { id = "batchNo", @Class = "form-control input-sm", @maxlength = "30" })
                                </div>
                                <div class="col-sm-3">
                                    <label for="form-field-select-1">
                                        <b>Insurance Details</b></label>
                                    @Html.TextBoxFor(model => model.InsuranceDetails, new { id = "insurancedetails", @Class = "form-control input-sm", @maxlength = "45" })
                                </div>
                                <div class="col-sm-3">
                                    <label for="form-field-select-1">
                                        <b>Last Worked</b></label>
                                    @Html.TextBoxFor(model => model.LastWorked, new { id = "lastWorked", @Class = "form-control input-sm", @maxlength = "45" })
                                </div>
                                <div class="col-sm-3">
                                    <label for="form-field-select-1">
                                        <b>Last worked location</b></label>
                                    @Html.TextBoxFor(model => model.WorkedLocation, new { id = "workedLocation", @Class = "form-control input-sm", @maxlength = "45" })
                                </div>
                            </div>
                            @*<div class="col-xs-12">
                            <div class="col-sm-2">
                                <label for="form-field-select-1">
                                    <b>Last Nett Salary</b>
                                </label>
                                @Html.TextBoxFor(model => model.LastDrawnNettSalary, new { id = "totalteachingexp", @Class = "form-control input-sm", @maxlength = "10" })
                            </div>
                            <div class="col-sm-2">
                                <label for="form-field-select-1">
                                    <b>Expected Salary</b></label>
                                @Html.TextBoxFor(model => model.ExpectedSalary, new { id = "syllabus", @Class = "form-control input-sm", @maxlength = "60" })
                            </div>
                            <div class="col-sm-4">
                                <label for="form-field-select-1">
                                    <b>If offer, how soon can you join us?</b></label>
                                @Html.TextBoxFor(model => model.JoiningDateOrDays, new { id = "subjects", @Class = "form-control input-sm", @maxlength = "60" })
                            </div>
                        </div>*@
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /.row -->
<script src="../../Scripts/StaffManagementScripts/ExperienceDetails.js" type="text/javascript"></script>
<script src="../../assets/js/AceJQMsg.js"></script>
<script src="../../Scripts/Attendance/Attendance.js"></script>
