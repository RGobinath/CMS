@{
    ViewBag.Title = "Transport Masters";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<!-- #section:basics/navbar.layout -->
<!-- /section:basics/navbar.layout -->
<script type="text/javascript">
    try { ace.settings.check('main-container', 'fixed') } catch (e) { }
</script>
<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
        <span>@Html.Raw(ViewBag.BreadCrumb)</span>
        <div class="row">
            <div class="col-xs-12">
                <!-- PAGE CONTENT BEGINS -->
                <div class="tabbable tabs-left">
                    <ul id="myTab" class="nav nav-tabs">
                        <li class="active"><a href="#Location" data-toggle="tab"><i class="ace-icon fa fa-location-arrow">
                        </i>&nbsp;Location</a> </li>
                        <li><a data-toggle="tab" href="#Route"><i class="ace-icon fa fa-road"></i>&nbsp;Route</a></li>
                        <li><a data-toggle="tab" href="#Driver"><i class="ace-icon fa fa-dot-circle-o"></i>&nbsp;Driver</a></li>
                        <li><a data-toggle="tab" href="#VehicleType"><i class="ace-icon fa fa-truck"></i>&nbsp;Vehicle
                            Type</a> </li>
                        <li><a data-toggle="tab" href="#Vendor"><i class="ace-icon fa fa-user"></i>&nbsp;Vendor</a>
                        </li>
                    </ul>
                    <div class="tab-content col-xs-6.col-sm-10">
                        <div class="tab-pane in active" id="Location">
                            @Html.Partial("LocationMaster")
                        </div>
                        <div class="tab-pane" id="Route">
                            @Html.Partial("RouteMaster")
                        </div>
                        <div class="tab-pane" id="Driver">
                            @Html.Partial("DriverMaster")
                        </div>
                        <div class="tab-pane" id="VehicleType">
                            @Html.Partial("VehicleTypeMaster")
                        </div>
                        <div class="tab-pane" id="Vendor">
                            @Html.Partial("TransportVendorMaster")
                        </div>
                    </div>
                </div>
                <!-- PAGE CONTENT ENDS -->
            </div>
            <!-- /.col -->
        </div>
    </div>
</div>
<script type="text/javascript">    window.jQuery || document.write("<script src='../assets/js/jquery.min.js'>" +
"<" + "/script>"); </script>
<script type="text/javascript">    if ('ontouchstart'
in document.documentElement) document.write("<script src='../assets/js/jquery.mobile.custom.min.js'>" +
"<" + "/script>"); </script>
<!-- inline scripts related to this page -->
<script src="../../Scripts/Transport/DriverMaster.js" type="text/javascript"></script>
<script src="../../Scripts/Transport/LocationMaster.js" type="text/javascript"></script>
<script src="../../Scripts/Transport/RouteMaster.js" type="text/javascript"></script>
