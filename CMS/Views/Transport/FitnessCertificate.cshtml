@model TIPS.Entities.TransportEntities.VehicleDetails
<div id="FitnessCertificate">
    <div class="row">
        <div class="col-xs-12 widget-container-col">
            <div class="widget-box widget-color-CustomizeBlue ">
                <div class="widget-header widget-header-flat widget-header-small">
                    <div class="PanelHeader">
                        Fitness Certificate Details</div>
                    <div class="widget-toolbar">
                        <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                        </a>
                    </div>
                </div>
                <div class="widget-body">
                    <div class="widget-main no-padding">
                        <div class="row" style="margin-top: 10px; margin-left: 3px; margin-bottom: 5px;">
                            <div class="col-xs-12">
                                <div class="col-sm-2 form-group has-info">
                                    <label for="form-field-select-1">
                                        <b>FC Date</b></label>
                                    <div class="input-group input-group-sm">
                                        @Html.TextBox("FCDate", "", new { id = "FC_FCDate", @class = "form-control date-picker" })
                                        <span class="input-group-addon"><i class="ace-icon fa fa-calendar"></i></span>
                                    </div>
                                </div>
                                <div class="col-sm-2 form-group has-info">
                                    <label for="form-field-select-1">
                                        <b>Next FC Date</b></label>
                                    <div class="input-group input-group-sm">
                                        @Html.TextBox("NextFCDate", "", new { id = "FC_NextFCDate", @class = "form-control date-picker" })
                                        <span class="input-group-addon"><i class="ace-icon fa fa-calendar"></i></span>
                                    </div>
                                </div>
                                <div class="col-sm-2 form-group has-info">
                                    <label for="form-field-select-1">
                                        <b>FC Cost</b></label>
                                    @Html.TextBox("FCCost", "", new { id = "FC_FCCost", @class = "form-control input-sm" })
                                </div>
                                <div class="col-sm-2 form-group has-info">
                                    <label for="form-field-select-1">
                                        <b>Description</b></label>
                                    @Html.TextBox("Description", "", new { id = "FC_Description", @class = "form-control input-sm" })
                                </div>
                                <div class="col-sm-2 form-group has-info">
                                    <label for="form-field-select-1">
                                        <b>FC Tax Valid Upto</b></label>
                                    <div class="input-group input-group-sm">
                                        @Html.TextBox("FCTaxValidUpto", "", new { @class = "form-control date-picker" })
                                        <span class="input-group-addon"><i class="ace-icon fa fa-calendar"></i></span>
                                    </div>
                                </div>
                                <div class="col-sm-2 form-group has-info">
                                    <label for="form-field-select-1">
                                        <b>FC Work Carried At</b></label>
                                    @Html.TextBox("FCWorkCarriedAt", "", new { id = "FC_FCWorkCarriedAt", @class = "form-control input-sm" })
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-sm-2 ">
                                    <label for="form-field-select-1">
                                        <b>RTO</b></label>
                                    @Html.TextBox("RTO", "", new { id = "FC_RTO", @class = "form-control input-sm" })
                                </div>
                                <div class="col-sm-2 ">
                                    <label for="form-field-select-1">
                                        <b>Driver</b></label>
                                    @Html.TextBox("Driver", "", new { @class = "form-control input-sm" })
                                </div>
                                <div class="col-sm-2">
                                    <label for="form-field-select-1">
                                        <b>Fitness Certificate</b></label>
                                    <input type="file" name="FCertificate" id="FCertificate" value="Browse" />
                                </div>
                                <div class="col-sm-2 ">
                                    <br />
                                    <button class="btn btn-success btn-info btn-block btn-sm" id="btnSave" type="button"
                                        onclick="FitnessCertificateDetailsCreate();return false;">
                                        <i class="ace-icon fa fa-floppy-o bigger-120 white"></i>&nbsp Save
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <!-- PAGE CONTENT BEGINS -->
            <table id="jqGridFitnessCertificate">
            </table>
            <div id="jqGridFitnessCertificatePager">
            </div>
            <!-- PAGE CONTENT ENDS -->
        </div>
        <!-- /.col -->
    </div>
</div>
<script src="@Url.Content("~/Scripts/Transport/FitnessCertificate.js")" type="text/javascript"></script>
