@{
    ViewBag.Title = "KioskEnquiry";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}

<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
      @*  <span>@Html.Raw(ViewBag.BreadCrumb)</span>*@
        <div class="row">
            <div class="col-xs-12 widget-container-col">
                <div class="widget-box widget-color-CustomizeBlue">
                    <div class="widget-header widget-header-flat widget-header-small">
                        <div class="PanelHeader">
                            <i class="fa fa-search"></i>&nbsp;Search
                        </div>
                        <div class="widget-toolbar">
                            <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                            </a>
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main no-padding">
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="col-sm-2">
                                    <label for="form-field-select-1">
                                        <b>Enquiry From Date</b></label>
                                    <div class="input-group input-group-sm">
                                        @Html.TextBox("AttendanceFromDate", "", new { @id = "Enquirydatefrom", placeholder = "dd/MM/yyyy", @Class = "maxLength form-control height-20", @onkeypress = "return isNumberKey(event, this)", @onkeyup = "return datecheck(event, this)", @maxlength = "10" })
                                        <span class="input-group-addon" style="cursor: pointer;"><i class="ace-icon fa fa-calendar AttFromdate"></i></span>
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <label for="form-field-select-1">
                                        <b>Enquiry To Date</b></label>
                                    <div class="input-group input-group-sm">
                                        @Html.TextBox("AttendanceToDate", "", new { @id = "Enquirytodate", placeholder = "dd/MM/yyyy", @Class = "maxLength form-control height-20", @onkeypress = "return isNumberKey(event, this)", @onkeyup = "return datecheck(event, this)", @maxlength = "10" })
                                        <span class="input-group-addon" style="cursor: pointer;"><i class="ace-icon fa fa-calendar AttTodate"></i></span>
                                    </div>
                                </div>
                                </div>
                            </div>
                        <div class="space-6"></div>
                          <div class="row">
                            <div class="col-xs-12">
                               
                                <div class="col-sm-1">
                                    <br />
                                    <button class="btn btn-success btn-block btn-sm" id="Search">
                                            <i class="ace-icon fa fa-search white"></i>Search
                                        </button>
                                </div>
                                <div class="col-sm-1">
                                    <br />
                                    <button class="btn btn-danger btn-block btn-sm" id="Reset">
                                            <i class="ace-icon fa fa-undo white"></i>Reset
                                        </button>
                                </div>
                            </div>
                            <div class="space-6"></div>
                        </div>
                        <div class="space-6"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="space-6"></div>
        <div class="row">
            <div class="col-sm-12">
                <table id="KioskEnquiryGridList">
                </table>
                <div id="KioskEnquiryGridListPager">
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/AdmissionScripts/KioskEnquiry.js"></script>
@*<script src="../../Scripts/Admission/KioskEnquiry.js"></script>*@