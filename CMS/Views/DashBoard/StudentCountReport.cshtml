@{
    ViewBag.Title = "Student DashBoard";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<!-- #section:basics/navbar.layout -->
<!-- /section:basics/navbar.layout -->
<!-- /For FusionCharts -->
<script src="@Url.Content("~/Scripts/FusionCharts1/FusionCharts.js")" type="text/javascript"></script>
<script type="text/javascript">
    try { ace.settings.check('main-container', 'fixed') } catch (e) { }
</script>
<script src="../../Scripts/DashboardScripts/StudentCountReport.js" type="text/javascript"></script>
<div class="page-content">
    <span>@Html.Raw(ViewBag.BreadCrumb)</span>
    <!-- /section:settings.box -->
    <div class="page-content-area">
        <!--Search Pannel-->
        <div class="row">
            <div class="col-xs-12 widget-container-col">
                <div class="widget-box widget-color-CustomizeBlue">
                    <div class="widget-header widget-header-flat widget-header-small">
                        <div class="PanelHeader">
                            <i class="ace-icon fa fa-search"></i>&nbsp;&nbsp;Student DashBoard</div>
                        <div class="widget-toolbar">
                            <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                            </a>
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main no-padding">
                            <div class="row" style="margin-top: 10px; margin-left: 3px; margin-bottom: 5px;">
                                <div class="col-xs-12">
                                    <div class="col-sm-2">
                                        <b>Select Academic Year </b>
                                        @Html.DropDownList("YearType", null, null, new { @Class = "form-control select-sm" })
                                         @Html.Hidden("BackUrl", Url.Action("StudentCountReport", "DashBoard")) 
                                    </div>
                                    <div class="col-sm-2">
                                        <b>Select Campus </b>
                                        @Html.DropDownList("CampusType", new SelectList(ViewBag.CampusType, "Name", "Name"), "All Campus", new { @Class = "form-control select-sm" })
                                        @*@Html.DropDownList("CampusType", null, null, new { @Class = "form-control select-sm" })*@
                                    </div>
                                    <div class="col-sm-1">
                                        <br />
                                        <button class="btn btn-success btn-block btn-sm" type="button" id="btnsubmit" value="Submit">
                                            <i class="ace-icon fa fa-check bigger-110"></i>Submit</button>&nbsp;&nbsp;
                                    </div>
                                    <div class="col-sm-1">
                                        <br />
                                        <button class="btn btn-danger btn-block btn-sm" type="button" id="reset">
                                            <i class="ace-icon fa fa-undo white"></i>Reset</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="space-2">
        </div>
        <div class="space-2">
        </div>
        <!-- PAGE CONTENT BEGINS -->
        <div class="row">
            <div class="col-xs-12">
                <div class="row">
                    <div class="col-sm-6">
                        <div id="EnquiryManagement">
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <table id="TableData">
                        </table>
                        <div id="TableDataPager">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
