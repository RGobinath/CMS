@{
    ViewBag.Title = "CallManagementHistory";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<!-- #section:basics/navbar.layout -->
<!-- /section:basics/navbar.layout -->

<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
        <span>@Html.Raw(ViewBag.BreadCrumb)</span>
        <div class="row">
            <div class="col-xs-12 widget-container-col">
                <div class="widget-box widget-color-CustomizeBlue">
                    @*<div class="widget-header widget-header-small">*@
                    <div class="widget-header widget-header-flat widget-header-small">
                        <div class="PanelHeader">
                            <i class="ace-icon fa fa-search"></i>&nbsp;Search Panel
                        </div>
                        <div class="widget-toolbar">
                            <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                            </a>
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main no-padding">
                            <div class="row" style="margin-top: 10px; margin-left: 3px; margin-bottom: 5px;">
                                <div class="col-xs-12">
                                    <div class="col-sm-2">
                                        <label for="form-field-select-1">
                                            <b>Campus</b></label>
                                        @Html.DropDownList("ddlCampus", new SelectList(Enumerable.Empty<SelectListItem>()), "Select", new { id = "ddlCampus", @Class = "form-control select-sm" })
                                    </div>
                                    <div class="col-sm-2">
                                        <label for="form-field-select-1">
                                            <b>Grade</b></label>
                                        @Html.DropDownList("ddlGrade", new SelectList(Enumerable.Empty<SelectListItem>()), "Select", new { id = "ddlGrade", @Class = "form-control select-sm" })
                                    </div>
                                    <div class="col-sm-2">
                                        <label for="form-field-select-1">
                                            <b>Section</b></label>
                                        @Html.DropDownList("ddlSection",
                                                                new SelectList(new[] { 
                                                                new { Text = "", Value = "Select", Seleced = "True" },
                                                                new { Text = "A", Value = "A", Seleced = "false" },
                                                                new { Text = "B", Value = "B", Seleced = "false" },
                                                                new { Text = "C", Value = "C", Seleced = "false" },
                                                                new { Text = "D", Value = "D", Seleced = "false" },
                                                                new { Text = "E", Value = "E", Seleced = "false" },
                                                                new { Text = "F", Value = "F", Seleced = "false" }
                                                                }, "Text", "Value", "Seleced"),
                                                                new { id="ddlSection",@Class = "form-control select-sm" })
                                    </div>
                                    <div class="col-sm-2">
                                        <label for="form-field-select-1">
                                            <b>Student Name</b></label>
                                        @Html.TextBox("txtStudentName", "", new { id = "txtStudentName", @class = "form-control select-sm " })
                                    </div>
                                    <div class="col-sm-2">
                                        <label for="form-field-select-1">
                                            <b>Student Number</b></label>
                                        @Html.TextBox("txtStudentNumber", "", new { id = "txtStudentNumber", @class = "form-control select-sm " })
                                    </div>
                                    <div class="col-sm-2">
                                        <label for="form-field-select-1">
                                            <b>Issue Number</b></label>
                                        @Html.TextBox("txtIssueNumber1", "", new { id = "txtIssueNumber1", @class = "form-control select-sm " })
                                    </div>
                                    <div class="col-sm-2">
                                        <label for="form-field-select-1">
                                            <b>Issue Group</b></label>
                                        @Html.DropDownList("ddlIssueGroup", new SelectList(Enumerable.Empty<SelectListItem>()), "Select", new { id = "ddlIssueGroup", @Class = "form-control select-sm" })
                                    </div>
                                    <div class="col-sm-2">
                                        <label for="form-field-select-1">
                                            <b>Issue Type</b></label>
                                        @Html.DropDownList("ddlIssueType", new SelectList(Enumerable.Empty<SelectListItem>()), "Select", new { id = "ddlIssueType", @Class = "form-control select-sm" })
                                    </div>
                                    @*<div class="col-sm-2">
                                        <label for="form-field-select-1">
                                            <b>Issue Date</b></label>
                                        <div class="input-group input-group-sm">
                                            @Html.TextBox("txtIssueDate", "", new { @class = "form-control" })
                                            <span class="input-group-addon"><i class="ace-icon fa fa-calendar"></i></span>
                                        </div>
                                    </div>*@
                                    @*<div class="col-sm-2">
                                        <label for="form-field-select-1">
                                            <b>Deleted Date</b></label>
                                        <div class="input-group input-group-sm">
                                            @Html.TextBox("txtDeletedDate", "", new { @class = "form-control" })
                                            <span class="input-group-addon"><i class="ace-icon fa fa-calendar"></i></span>
                                        </div>
                                    </div>*@
                                    <div class="col-sm-1">
                                        <br />
                                        <button class="btn btn-primary btn-info btn-block btn-sm" id="btnSearch">
                                            <i class="ace-icon fa fa-search white"></i>Search
                                        </button>
                                    </div>
                                    <div class="col-sm-1">
                                        <br />
                                        <button class="btn btn-danger btn-warning btn-block btn-sm" id="btnReset">
                                            <i class="ace-icon fa fa-undo white"></i>Reset
                                        </button>
                                    </div>
                                    <div class="col-sm-1">
                                        <br />
                                        <button class="btn btn-warning btn-warning btn-block btn-sm" id="btnDelete">
                                            <i class="ace-icon fa fa-trash-o white"></i>Delete
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="space-2">
        </div>
        <div class="row">
            <div class="col-xs-12">
                <!-- PAGE CONTENT BEGINS -->
                <table id="grid-table">
                </table>
                <div id="grid-pager">
                </div>
                <!-- PAGE CONTENT ENDS -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.page-content-area -->
</div>
<div id="DeleteCallMgmnt"></div>
<script type="text/javascript">
    window.jQuery || document.write("<script src='../assets/js/jquery.min.js'>" + "<" + "/script>");
</script>
<script type="text/javascript">
    if ('ontouchstart' in document.documentElement) document.write("<script src='../assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");
</script>
<!-- inline scripts related to this page -->
<script src="@Url.Content("~/Scripts/Home/CallManagementHistory.js")" type="text/javascript"></script>
