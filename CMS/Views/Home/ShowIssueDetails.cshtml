@model TIPS.Entities.CallManagement
@{
    ViewBag.Title = "Show Issue Details";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<!-- #section:basics/navbar.layout -->
<!-- /section:basics/navbar.layout -->
<script type="text/javascript">
    try { ace.settings.check('main-container', 'fixed') } catch (e) { }
</script>
@using (Html.BeginForm("CallForward", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="page-content">
        <!-- /section:settings.box -->
        <div class="page-content-area">
            <span>@Html.Raw(ViewBag.BreadCrumb)</span>
            <div class="space-2">
            </div>
            <div class="row">
                <div class="col-xs-12 widget-container-col">
                    <div class="widget-box widget-color-CustomizeBlue">
                        <div class="widget-header widget-header-flat widget-header-small">
                            <div class="PanelHeader">
                                <i class="ace-icon fa fa-list"></i>&nbsp; Basic Details</div>
                            <div class="widget-toolbar">
                                <a href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                                </a>
                            </div>
                        </div>
                        <div class="widget-body">
                            <div class="widget-main no-padding">
                                <div class="row">
                                    <div class="col-xs-12">
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Issue Number </b>
                                            </label>
                                            <br />
                                            @Model.IssueNumber
                                            @Html.HiddenFor(model => model.Id, new { id = "Id" })
                                            @Html.HiddenFor(model => model.IssueNumber, "")
                                            @Html.HiddenFor(model => model.Description, "")
                                            @Html.HiddenFor(model => model.CallPhoneNumber, "")
                                            @Html.HiddenFor(model => model.StudentName, "")
                                            @Html.HiddenFor(model => model.CallFrom, "")
                                            @Html.HiddenFor(model => model.StudentNumber, "")
                                            @Html.HiddenFor(model => model.StudentType, "")
                                            @Html.HiddenFor(model => model.School, "")
                                            @Html.HiddenFor(model => model.Section, "")
                                            @Html.HiddenFor(model => model.Approver, "")
                                            @Html.HiddenFor(model => model.InstanceId, "")
                                            @Html.HiddenFor(model => model.Email, "")
                                            @Html.HiddenFor(model => model.UserInbox, "")
                                            @Html.HiddenFor(model => model.IssueDate, "")
                                            @Html.HiddenFor(model => model.UserRoleName, "")
                                            @Html.HiddenFor(model => model.Grade, "")
                                            @Html.Hidden("hidStatus", "", new { id = "hidStatus", @Value = ViewBag.status })
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Date</b>
                                            </label>
                                            <br />
                                            @String.Format("{0:dd-MMM-yyyy }", @Model.IssueDate)
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Processed By</b>
                                            </label>
                                            <br />
                                            @Model.UserInbox
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>User Role</b>
                                            </label>
                                            <br />
                                            @Model.UserRoleName
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Issue Status</b>
                                            </label>
                                            <br />
                                            @Model.ActivityFullName
                                            @Html.HiddenFor(model => model.Status, new { id = "Status", @Value = Model.Status })
                                            @Html.HiddenFor(model => model.ActivityFullName)
                                        </div>
                                    </div>
                                    <div class="space-2">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 widget-container-col">
                    <div class="widget-box widget-color-CustomizeBlue">
                        <div class="widget-header widget-header-flat widget-header-small">
                            <div class="PanelHeader">
                                <i class="ace-icon fa fa-list"></i>&nbsp;Issue Details</div>
                            <div class="widget-toolbar">
                                <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                                </a>
                            </div>
                        </div>
                        <div class="widget-body">
                            <div class="widget-main no-padding">
                                <div class="row">
                                    <div class="col-xs-12">
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Campus</b>
                                            </label>
                                            <br />
                                            @Model.BranchCode
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Information For</b>
                                            </label>
                                            <br />
                                            @Model.InformationFor
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Leave Type</b>
                                            </label>
                                            <br />
                                            @Model.LeaveType
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Issue Group</b>
                                            </label>
                                            <br />
                                            @Model.IssueGroup
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Issue Type</b>
                                            </label>
                                            <br />
                                            @Model.IssueType
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Student Number</b>
                                            </label>
                                            <br />
                                            @Model.StudentNumber
                                        </div>
                                    </div>
                                </div>
                                <div class="space-2">
                                </div>
                                <div class="row">
                                    <div class="col-xs-12">
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Student Name</b>
                                            </label>
                                            <br />
                                            @Model.StudentName
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Caller Phone Number</b>
                                            </label>
                                            <br />
                                            @Model.CallPhoneNumber
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Call from</b>
                                            </label>
                                            <br />
                                            @Model.CallFrom
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Caller Name</b>
                                            </label>
                                            <br />
                                            @Model.CallerName
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>School</b>
                                            </label>
                                            <br />
                                            @Model.School
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Grade</b>
                                            </label>
                                            <br />
                                            @Model.Grade
                                        </div>
                                    </div>
                                </div>
                                <div class="space-2">
                                </div>
                                <div class="row">
                                    <div class="col-xs-12">
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Section</b>
                                            </label>
                                            <br />
                                            @Model.Section
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Email</b>
                                            </label>
                                            <br />
                                            @Model.Email
                                        </div>
                                        <div class="col-sm-1">
                                            <br />
                                            <button type="button" id="Back" class="btn btn-purple btn-block btn-sm">
                                                Back to List</button>
                                        </div>
                                    </div>
                                </div>
                                <div class="space-2"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 widget-container-col">
                    <div class="widget-box widget-color-CustomizeBlue">
                        <div class="widget-header widget-header-flat widget-header-small">
                            <div class="PanelHeader">
                                <i class="ace-icon fa fa-list"></i>&nbsp;Description Details</div>
                            <div class="widget-toolbar">
                                <a href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i></a>
                            </div>
                        </div>
                        <div class="widget-body">
                            <div class="widget-main no-padding">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="col-sm-4">
                                    <label for="form-field-select-1">
                                        <b>Issue Description</b>
                                    </label>
                                    @Html.TextAreaFor(model => model.Description, new { rows = 2, cols = 60, id = "txtDescription", @class = "form-control input-sm" })
                                    <label for="form-field-select-1">
                                        <b>Issue Resolution Comments</b>
                                    </label>
                                    @Html.TextAreaFor(model => model.Resolution, new { rows = 2, cols = 60, id = "txtResolution", @class = "form-control input-sm" })
                                </div>
                                <div class="col-sm-8">
                                    <div class="row">
                                        <div class="col-xs-12">
                                            Rejection Grid
                                        </div>
                                    </div>
                                    <div class="space-2">
                                    </div>
                                    <div class="row">
                                        <div class="col-xs-12" id="jqgrid">
                                            @if (Model != null && (Model.Status != "LogIssue"))
                                            {   
                                                <table id="DescriptionForSelectedId">
                                                </table>
                                                <div id="DescriptionForSelectedIdPager">
                                                </div>
                                            }
                                        </div>
                                    </div>
                                    <div class="space-2">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
<script src='@Url.Content("/Scripts/Home/ShowIssueDetails.js")' type="text/javascript"></script>
<script type="text/javascript">    window.jQuery || document.write("<script src='../assets/js/jquery.min.js'>" + "<" + "/script>"); </script>
<script type="text/javascript">    if ('ontouchstart' in document.documentElement) document.write("<script src='../assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>"); </script>