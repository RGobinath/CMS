@model TIPS.Entities.AdmissionManagement
<link href="@Url.Content("~/App_Themes/smoothness/jquery-ui-1.8.22.custom.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/App_Themes/Default/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
<script src="../../Scripts/jquery-1.7.2.js" type="text/javascript"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jqGridJs/i18n/grid.locale-en.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jqGridJs/jquery.jqGrid.min.js")"></script>
<style>
    .InnerTable
    {
        font: normal 11px Verdana,Arial,sans-serif;
        border: none;
        text-align: left;
    }
    .InnerTable th
    {
        border: none;
        border-color: Black;
        border-collapse: collapse;
        font: normal 11px Verdana,Arial,sans-serif;
    }
    .InnerTable td
    {
        border: none;
        border-color: Black;
        border-collapse: collapse;
        font: normal 11px Verdana,Arial,sans-serif;
    }
</style>
<script type="text/javascript">
    $(document).ready(function () {

        jQuery("#CancelSectionAllocationList").jqGrid({
            url: '/Home/AdmissionManagementListJqGrid',
            datatype: 'json',
            mtype: 'GET',
            height: '190',
            width: '440',

            colNames: ['Application No', 'Pre-Reg No', 'Name of Applicant', 'Campus', 'Status of ', 'Applied Date', 'Approve'],
            colModel: [

              { name: 'ApplicationNo', index: 'ApplicationNo', width: 40, align: 'left', sortable: true },
              { name: 'PreRegNo', index: 'PreRegNo', width: 40, align: 'left', sortable: true },
              { name: 'ApplicantName', index: 'ApplicantName', width: 40, align: 'left', sortable: true },
              { name: 'Grade', index: 'Grade', width: 40, align: 'left', sortable: true },
              { name: 'Status', index: 'Status', width: 40, align: 'left', sortable: true },
              { name: 'AppliedDate', index: 'AppliedDate', width: 40, align: 'left', sortable: true },
              { name: 'my_clickable_checkbox', index: 'my_clickable_checkbox', sortable: true, formatter: "checkbox", formatoptions: { disabled: false }, editable: true, edittype: "checkbox", width: 50, align: 'left', sortable: true }
              ],
            pager: '#CancelSectionAllocationListPager',
            rowNum: '10',
            rowList: [5, 10, 20, 50, 100, 150, 200 ],
            sortname: '',
            sortorder: "",
            viewrecords: true,
            multiselect: true,
            caption: 'Inbox'
        });
        jQuery("#CancelSectionAllocationList").jqGrid('navGrid', '#CancelSectionAllocationListPager', {});

    });
</script>
@using (Html.BeginForm("CancelSectionAllocation", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <h3>
        Section Un Grouping</h3>
    <hr />
    <table width="100%" class="InnerTable" cellpadding="4" cellspacing="4">
        <tr>
            <td>
                <table id="CancelSectionAllocationList" class="WithBorder" width="100%">
                </table>
                <div id="CancelSectionAllocationListPager">
                </div>
            </td>
        </tr>
    </table>
}
