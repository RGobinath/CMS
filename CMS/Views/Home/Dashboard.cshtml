@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<!-- #section:basics/navbar.layout -->
<!-- /section:basics/navbar.layout -->
<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
        @using (Html.BeginForm("Home", "Home"))
        {
            <span>@Html.Raw(ViewBag.BreadCrumb)</span>
            <div class="row">
                <div class="space-2">
                </div>
                <div class="col-xs-12 widget-container-col">
                    <div class="widget-box widget-color-CustomizeBlue">
                        <div class="widget-header widget-header-flat widget-header-small">
                            <div class="PanelHeader">
                                <i class="menu-icon fa fa-tachometer"></i>&nbsp;Dashboard</div>
                            <div class="widget-toolbar">
                                <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                                </a>
                            </div>
                        </div>
                        <div class="widget-body">
                            <div class="widget-main no-padding">
                                <div class="space-2">
                                </div>
                                <div class="row" style="margin-top: 10px; margin-left: 3px; margin-bottom: 5px;">
                                    <div class="col-xs-12">
                                        <div class="row">
                                            <div class="col-sm-2" style="padding: 20px;">
                                                <b>Campus:</b>
                                                @Html.DropDownList("ddlCampus", new SelectList(Enumerable.Empty<SelectListItem>()), "----Select----", new { @Class = "form-control select-sm" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-12">
                                    <div class="row">
                                        <div class="col-sm-4">
                                            <center>
                                                <b>Non Completed Issue Count </b>
                                                <table>
                                                    <tr>
                                                        <td id="chart2">
                                                            <center>
                                                                <div id="ColumnChartDiv">
                                                                </div>
                                                            </center>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </center>
                                        </div>
                                        <div class="col-sm-4">
                                            <center>
                                                <b>Weekly Issue Status </b>
                                                <div id="WeeklyIssueStatus1Div">
                                                </div>
                                            </center>
                                        </div>
                                        <div class="col-sm-4">
                                            <center>
                                                <b>Issue Count by Issue Group</b>
                                                <table>
                                                    <tr>
                                                        <td id="chart1">
                                                            <div id="FunnelChartDiv">
                                                            </div>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </center>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
    <!-- /.page-content-area -->
</div>
<script src="@Url.Content("~/Scripts/FusionCharts1/FusionCharts.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Home/Dashboard.js")" type="text/javascript"></script>
