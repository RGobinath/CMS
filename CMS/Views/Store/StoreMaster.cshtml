@{
    ViewBag.Title = "Store Masters";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="TIPS Transport Master" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<!-- #section:basics/navbar.layout -->
<!-- /section:basics/navbar.layout -->
<script type="text/javascript">
    try { ace.settings.check('main-container', 'fixed') } catch (e) { }
</script>
@using (Html.BeginForm("StoreMaster", "Store", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="page-content">
        <!-- /section:settings.box -->
        <div class="page-content-area">
            <span>@Html.Raw(ViewBag.BreadCrumb)</span>
            <div class="row">
                <div class="col-xs-12">
                    <!-- PAGE CONTENT BEGINS -->
                    <div class="tabbable tabs-left">
                        <ul id="myTab" class="nav nav-tabs">
                            <li class="active"><a href="#StoreUnit" data-toggle="tab"><i class="ace-icon fa fa-coffee"></i>&nbsp;Unit of Measurement</a> </li>
                            <li><a data-toggle="tab" href="#StoreMaterialGroup"><i class="ace-icon fa fa-cog"></i>&nbsp;Material Group</a></li>
                            <li><a data-toggle="tab" href="#StoreMaterialSubGroup"><i class="ace-icon fa fa-cogs"></i>&nbsp;Material Sub Group</a></li>
                            <li><a data-toggle="tab" href="#StoreSKU"><i class="ace-icon fa fa-book"></i>&nbsp;SKU</a> </li>
                            <li><a data-toggle="tab" href="#StorerFunctionaries"><i class="ace-icon fa fa-glass"></i>&nbsp;Storer Functionaries</a>
                            </li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane in active" id="StoreUnit">
                                @Html.Partial("StoreUnitsMaster")
                            </div>
                            <div class="tab-pane" id="StoreMaterialGroup">
                                @Html.Partial("StoreMaterialGroupMaster")
                            </div>
                            <div class="tab-pane" id="StoreMaterialSubGroup">
                                @Html.Partial("StoreMaterialSubGroupMaster")
                            </div>
                            <div class="tab-pane" id="StoreSKU">
                                @Html.Partial("StoreSKUMaster")
                            </div>
                            <div class="tab-pane" id="StorerFunctionaries">
                                @Html.Partial("StorerFunctionaries")
                            </div>
                        </div>
                    </div>
                    <!-- PAGE CONTENT ENDS -->
                </div>
                <!-- /.col -->
            </div>
        </div>
    </div>
}
<script type="text/javascript">    window.jQuery || document.write("<script src='../assets/js/jquery.min.js'>" + "<" + "/script>"); </script>
<script type="text/javascript">    if ('ontouchstart' in document.documentElement) document.write("<script src='../assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>"); </script>
<!-- inline scripts related to this page -->
<script src="@Url.Content("~/Scripts/Store/StoreMaster.js")" type="text/javascript"></script>

