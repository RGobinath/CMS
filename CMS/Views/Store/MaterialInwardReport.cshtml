@{
    ViewBag.Title = "MaterialInwardReport";
    Layout = "~/Views/Shared/NewAcePartialViewMaster.cshtml";
                        // Layout = null;
}

<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta charset="utf-8" />
<meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<!-- #section:basics/navbar.layout -->
<!-- /section:basics/navbar.layout -->
<script type="text/javascript">
    try { ace.settings.check('main-container', 'fixed') } catch (e) { }
</script>
<script src="../../assets/js/AceJQMsg.js" type="text/javascript"></script>
 
@using (Html.BeginForm("MaterialInwardReport", "Store", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
        <span>@Html.Raw(ViewBag.BreadCrumb)</span>
        <!--Search Pannel-->
        <div class="row">
            <div class="col-xs-12 widget-container-col">
                <div class="widget-box widget-color-CustomizeBlue">
                    <div class="widget-header widget-header-flat widget-header-small">
                        <div class="PanelHeader">
                            <i class="ace-icon fa fa-search"></i>&nbsp;Search Panel</div>
                        <div class="widget-toolbar">
                            <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                            </a>
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main no-padding">
                            <div class="row" style="margin-top: 10px; margin-left: 3px; margin-bottom: 5px;">
                                <div class="col-xs-12">
                                 
                                    <div class="col-sm-2">
                                        <b>From Date</b>
                                       @Html.TextBox("txtFromCreatedDate", "", new { @class = "from_date", @readonly = true })
                                        @Html.Hidden("BackUrl", Url.Action("MaterialInwardMonthlyReport", "Store"))
                                    </div>
                                    <div class="col-sm-2">
                                        <b>To Date</b>
                                        @Html.TextBox("txtToCreatedDate", "", new { @class = "to_date", @readonly = true })
                                
                                    </div>
                                    <div class="col-sm-1">
                                        <br />
                                        <button class="btn btn-success btn-block btn-sm" type="button" id="btnSearch" value="Search">
                                            <i class="ace-icon fa fa-search white"></i>Search</button>
                                    </div>
                                    <div class="col-sm-1">
                                        <br />
                                        <button class="btn btn-danger btn-block btn-sm" type="button" id="btnReset" value="Reset">
                                            <i class="ace-icon fa fa-undo white"></i>Reset</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /.page-content-area -->
                    </div>
                </div>
            </div>
        </div>
        <div class="space-2">
        </div>
        <div class="row">
            <div class="col-xs-12 widget-container-col">
                <!-- PAGE CONTENT BEGINS -->
                <table id="MaterialInwardReportList" class="InnerTable">
                </table>
                <div id="MaterialInwardReportListPager">
                </div>
                <!-- PAGE CONTENT ENDS -->
            </div>
            <!-- /.col -->
        </div>
    </div>
</div>
}
<script src="@Url.Content("/Scripts/Store/MaterialInwardReport.js")" type="text/javascript"></script>
