<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta charset="utf-8" />
    <meta name="description" content="Dynamic tables and grids using jqGrid plugin" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    <link href="@Url.Content("~/assets/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/Customize.ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.2.js")" type="text/javascript"></script>
    
    <script src='@Url.Content("~/Scripts/JQueryUI/jquery-ui-1.8.22.custom.min.js")'type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/json2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/js/jqGrid/i18n/grid.locale-en.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/js/jqGrid/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/BootStrap/bootstrap-session-timeout.js")" type="text/javascript"></script>
    <!-- bootstrap & fontawesome -->
    <link href="@Url.Content("~/assets/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/Customize.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/assets/css/font-awesome.min.css")" type="text/css" />
    <!-- page specific plugin styles -->
    <link rel="stylesheet" href="@Url.Content("~/assets/css/jquery-ui.min.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/assets/css/datepicker.css")" type="text/css" />    

    <!-- text fonts -->
    <link rel="stylesheet" href="@Url.Content("~/assets/css/ace-fonts.css")" type="text/css" />
    <!-- ace styles -->
    <link rel="stylesheet" href="@Url.Content("~/assets/css/ace.min.css")" id="main-ace-style" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/assets/css/Customize.ace.min.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/assets/css/ace-skins.min.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/assets/css/ace-rtl.min.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/assets/AdminTemplate.css")" type="text/css" />
    <script src="@Url.Content("~/assets/js/ace-extra.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/js/bootstrap.min.js")" type="text/javascript"></script>
    <!-- page specific plugin scripts -->
    <script src="@Url.Content("~/assets/js/date-time/bootstrap-datepicker.min.js")" type="text/javascript"></script>    
    <!-- ace scripts -->
    <script src="@Url.Content("~/assets/js/ace-elements.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/js/ace.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ApplicationScript.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/js/AceJQMsg.js")"  type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FusionCharts/FusionCharts.js")"  type="text/javascript"></script>
    <script src="../../Scripts/ajaxfileupload.js" type="text/javascript"></script>

    <script type="text/javascript" language="javascript">

        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-76608362-1', 'auto');
        ga('send', 'pageview');



        $(document).ready(function () {
            var defFunc = function defaultFunc(e) {
                if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
                    $("#btnsearch").click();
                    return false;
                }
                return true;
            }
            $('input:text', '#srchKeyPress').keypress(defFunc);

            /////
            $.sessionTimeout({
                message: 'Your session will be locked in one minute.',
                keepAliveUrl: '@Url.Action("KeepAlive", "Home")',
                logoutUrl: '@Url.Action("LogOff", "Account")',
                redirUrl: '@Url.Action("LogOff", "Account")',
                warnAfter: 900000,  // 15 minutes
                redirAfter: 1200000   // 20 minutes
            });

            ////
        });
    </script>
</head>
<body class="light-login">
