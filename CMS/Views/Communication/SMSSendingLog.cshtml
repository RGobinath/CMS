@{
    ViewBag.Title = "SMS Sending Log";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}
<script src="@Url.Content("~/Scripts/Notification/SMSSendingLog.js")" type="text/javascript"></script>

<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
        @Html.Hidden("BackUrl", Url.Action("BulkSMSRequest", "Communication"))
        <span>@Html.Raw(ViewBag.BreadCrumb)</span>
        <div class="row">
            <div class="col-xs-12 widget-container-col">
                <div class="widget-box widget-color-CustomizeBlue">
                    <div class="widget-header widget-header-flat widget-header-small">
                        <div class="PanelHeader">
                            <i class="fa fa-mobile fa-lg "></i>&nbsp;SMS Sending Log</div>
                        <div class="widget-toolbar">
                            <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                            </a>
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main no-padding">
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="space-2">
                                </div>
                                <div class="col-sm-2">
                                    <button type="button" class="btn btn-mini btn-success" id="Send" value="Compose Sms"
                                        onclick="uploaddoc2()">
                                        <i class="fa fa-paper-plane" style="vertical-align: middle">&nbsp;</i>Send Sms
                                    </button>
                                    <div class="space-2">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <table id="jqBulkSMSRequest">
                </table>
                <div id="jqBulkSMSRequestPager">
                </div>
            </div>
        </div>
    </div>
</div>
