@{
    Layout = "~/Views/Shared/NewAcePartialViewMaster.cshtml";
}
<div class="row">
    <div class="col-xs-12 widget-container-col">
        <div class="widget-box widget-color-CustomizeBlue">
            <div class="widget-header widget-header-flat widget-header-small">
                <div class="PanelHeader">
                    <i class="ace-icon fa fa-search"></i>&nbsp;Search Pannel
                </div>
                <div class="widget-toolbar">
                    <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                    </a>
                </div>
            </div>
            <div class="widget-body">
                <div class="widget-main no-padding">
                    <div class="row" style="margin-top: 10px; margin-left: 3px; margin-bottom: 5px;">
                        <div class="col-xs-12">
                            <div class="col-sm-2">
                                <label for="form-field-select-1"><b>Search By</b></label>
                               @Html.DropDownList("ddlSearchBy", new SelectList(new[]
                                                    {
                                                        new{Value="",Text="Select One"},
                                                        new{Value="Module",Text="Module"},
                                                        new{Value="Number",Text="Reference Number"},
                                                        new{Value="PreRegNum",Text="Admission Number"},
                                                        new{Value="Description",Text="Description"},
                                                    },
                                                    "Value", "Text", ""), new { @Class = "form-control select-sm" })
                            </div>
                            <div class="col-sm-2">
                                <label for="form-field-select-1"><b>Search For</b></label>
                                @Html.TextBox("txtSearch", "", new { @class = "form-control input-sm" })
                            </div>
                            <div class="col-sm-1">
                                <label for="form-field-select-1"><b></b></label>
                                <button type="button" class="btn btn-success btn-info btn-block btn-sm" id="btnSearch">
                                    
                                    <i class="fa fa-search fa-fw"></i>Search
                                </button>
                            </div>
                              <div class="col-sm-1">
                                <label for="form-field-select-1"><b></b></label>
                                <button type="button" class="btn btn-danger btn-block btn-sm" id="btnReset">
                                    
                                    <i class="ace-icon fa fa-undo white"></i>Reset
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-xs-12">
        <table id="jqGridInboxMaster">
        </table>
        <div id="jqGridInboxPager">
        </div>
        <!-- PAGE CONTENT ENDS -->
    </div>
    <!-- /.col -->
</div>
<div id="Activities"></div>

<script src="../../Scripts/Inbox/Inbox.js"></script>






