@model TIPS.Entities.Assess.Assess360Assignment
@{
    ViewBag.Title = "Assess360AssignmentCrossCheck";
    Layout = "~/Views/Shared/NewAceMaster.cshtml";
}
<script src="@Url.Content("~/Scripts/Assess360/Assess360AssignmentCrossCheck.js")" type="text/javascript"></script>
<div class="page-content">
    <!-- /section:settings.box -->
    <div class="page-content-area">
        <span>@Html.Raw(ViewBag.BreadCrumb)</span>
        <div class="row">
            <div class="col-xs-12 widget-container-col">
                <div class="widget-box widget-color-CustomizeBlue">
                    <div class="widget-header widget-header-flat widget-header-small">
                        <div class="PanelHeader">
                            <i class="ace-icon fa fa-search"></i>&nbsp;Search Criteria
                        </div>
                        <div class="widget-toolbar">
                            <a data-action="collapse" href="#"><i class="1 ace-icon fa fa-chevron-down fa-6x"></i>
                            </a>
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main no-padding">
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="row">
                                    <div class="col-xs-12">
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Campus Name</b></label>
                                            @Html.DropDownList("Campus", new SelectList(Enumerable.Empty<SelectListItem>()), "----Select----", new { @Class = "form-control select-sm" })
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Grade</b></label>
                                            @Html.DropDownList("Grade",
                    new SelectList(new[] { 
                        new {Value = "", Text = "Select" },
                        new {Value = "VI",  Text = "VI" },
                        new {Value = "VII", Text = "VII" },
                        new {Value = "VIII", Text = "VIII" },
                        new {Value = "IX", Text = "IX"},
                        new {Value = "X", Text = "X"}
               }, "Value", "Text"), new { @Class = "form-control select-sm" })
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Section</b></label>
                                            @Html.DropDownList("Section", new SelectList(new[] { 
                        new { Value = "",Text = "Select" },
                        new { Value = "A", Text = "A"},
                        new { Value = "B", Text = "B" },
                        new { Value = "C", Text = "C" },
                        new { Value = "D", Text = "D"  },
                        new { Value = "E", Text = "E"  },
                        new { Value = "F", Text = "F"  }
               }, "Value", "Text"), new { @Class = "form-control select-sm" })
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Staff</b></label>
                                            @Html.DropDownList("Staff", new SelectList(Enumerable.Empty<SelectListItem>()), "----Select----", new { @Class = "form-control select-sm" })
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Subject</b></label>
                                            @Html.DropDownListFor(model => model.Subject, new SelectList(Enumerable.Empty<SelectListItem>()), "Select", new { id = "Subject", @Class = "form-control select-sm" })
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="form-field-select-1">
                                                <b>Assessment Type</b></label>
                                            @Html.DropDownList("AssessGroup", new SelectList(ViewBag.AssessGroupMaster, "Id", "GroupName"), "Select", new { @Class = "form-control select-sm" })
                                        </div>
                                    </div>
                                </div>
                                <div class="space-2">
                                </div>
                                <div class="row">
                                    <div class="col-xs-12">
                                        <div class="col-sm-2" id="tdSemester">
                                            <label for="form-field-mask-1">
                                                <b>Semester Type</b></label>
                                            @Html.DropDownList("Semester", new SelectList(new[] { 
                                                                    new { Text = "", Value = "Select", Seleced = "True" },
                                                                    new { Text = "Sem I", Value = "Sem I", Seleced = "false" },
                                                                    new { Text = "Sem II", Value = "Sem II", Seleced = "false" },
                                                                    new { Text = "Sem III", Value = "Sem III", Seleced = "false" },
                                                                    new { Text = "Model I",Value = "Model I",Seleced = "false"  },
                                                                    new { Text = "Model II",Value = "Model II",Seleced = "false" },
                                                                    new { Text = "Model III",Value = "Model III",Seleced = "false" },
                                                                    new { Text = "Model IV",Value = "Model IV",Seleced = "false" },
                                                                    }, "Text", "Value", "Seleced"), new { @Class = "form-control select-sm" })
                                        </div>
                                        <div class="col-sm-1">
                                            <div class="space-2">
                                            </div>
                                            <button class="btn btn-success btn-block btn-sm" id="Search" value="Search" type="button">
                                                <i class="ace-icon fa fa-search white"></i>Search</button>
                                        </div>
                                        <div class="col-sm-1">
                                            <div class="space-2">
                                            </div>
                                            <button class="btn btn-danger btn-block btn-sm" id="btnReset" value="Reset">
                                                <i class="ace-icon fa fa-undo white"></i>&nbsp;Reset
                                            </button>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="space-2">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <table id="Assess360AssignmentList">
                </table>
                <div id="Assess360AssignmentListPager">
                </div>
            </div>
        </div>
    </div>
</div>
<div id="EnteredListDiv"></div>
<div id="NotEnteredListDiv"></div>
